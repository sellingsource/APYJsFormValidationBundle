function NotBlank(field, params)
{
    var value;
    if (field.type == 'checkbox' && !field.checked )
    {
        value = '';
    }
    else
    {
        value = field && field.nodeName ? {{ getJsFormElementValue('field') }} : field;
    }

    if (isNotDefined(value)) {
        return getComputeMessage(params.message);
    }

    return true;
}